        function openProjectDetail(data) {
            console.log(data, 'data edit candidate');
            $('#container').find('img').remove();
            $.ajax({
                url: "/getGovtProjDetail",
                data: { _id: data },
                type: "GET",
                contentType: "application/json",
                success: function (response) {
                    console.log(response)
                    $(".userId").html(response._id);
                    $(".document").html('<img style="height: 40px;" src="/upload/' + response.data.document + '">');
                    $(".example-title-input").html(response.data.title);
                    $(".example-summary-input").html(response.data.summary);
                    $(".example-category-input").html(response.data.category);
                    $(".example-description-input").html(response.data.description);
                    $(".example-currency-input").html(response.data.currency);
                    $(".example-price-input").html(response.data.price);
                    $(".example-skill-input").html(response.data.skill);
                    $(".example-date-input").html(response.data.date);
                },
                error: function (xhr, ajaxOptions, thrownError) {
                    console.log(thrownError, 'error')
                    $(".example-text-input").html('');
                    $(".example-text-input").html('');
                    $(".example-type-input").html('');
                    $(".example-address-input").html('');
                }
            });
        }

        $(document).on('input', '#example-price-input', function() {
                 this.value = this.value.replace(/[^0-9 _]/g, '');
            });
